<div class="d-flex justify-content-center">
   <h4>化粧品新規登録</h4>
</div>

<div class="d-flex justify-content-center">
<p>化粧品画像</p>
</div>

<%= form_with model: @cosmetic, url:admin_cosmetics_path do |f| %>
<div class="d-flex justify-content-center">
  <%= f.file_field :image, accept: "image/*" %>
</div>
<div class="d-flex justify-content-center">
<p>化粧品名</p>
</div>
<div class="d-flex justify-content-center">
<%= f.text_field :name %>
</div>
<div class="d-flex justify-content-center">
<p>ブランド</p>
</div>
<div class="d-flex justify-content-center">
<%= f.text_field :brand %>
</div>

<div class="d-flex justify-content-center">
<%= f.submit '新規登録', class:"btn btn-primary" %>
<% end %>
</div>