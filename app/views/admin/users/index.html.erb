<div class="container">
  <div class="row">
    <div class="col-12">
      <h4>会員一覧</h4>
      
<table class="table">
  <thead class="thead-light">
    <tr>
        <th scope="col">会員ID</th>
        <th scope="col">ニックネーム</th>
        <th scope="col">ステータス</th>
    </tr>
  </thead>
  <tbody>
      <% @users.each do |user| %>
      <tr>
          <td><%= user.id %></td>
          <td><%= link_to user.nickname, admin_user_path(user.id) %></td>
              <td><% if user.is_deleted %>
                 <span style="color: #808080;">退会済</span>
                <% else %>
                 <span style="color: blue;">有効</span>
                <% end %>
              </td>
        </tr>
      <% end %>
  </tbody>
  </table>
    <div class="d-flex justify-content-center">
  <%= paginate @users %>
 </div>
 </div>
 </div>
 </div>