<div class="container">
  <div class="row">
    <div class="col-8">
      <h4>会員一覧</h4>
      
<table class="table">
  <thead class="thead-light">
    <tr>
        <th scope="col">会員ID</th>
        <th scope="col">ニックネーム</th>
    </tr>
  </thead>
  <tbody>
      <% @users.each do |user| %>
      <tr>
          <td><%= user.id %></td>
          <td><%= link_to user.nickname, admin_user_path(user.id) %></td>
      </tr>
      <% end %>
  </tbody>
  </table>
    <div class="d-flex justify-content-center">
  <%= paginate @users %>
 </div>
 </div>
 </div>
 </div>