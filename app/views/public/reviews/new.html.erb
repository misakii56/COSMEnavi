 <div class="container">
  <div class="row">
    <div class="col-12">
        <h4><span>レビューを投稿する</h4></span>
    </div>
  </div>
    <div class="new">
      <%= form_with model: @review, url: cosmetic_reviews_path(@cosmetic.id) do |f| %>
      <div class="new01">評価</div>
      <div id="post_raty"></div>
      <div class="new01">レビュー</div>
      <%= f.text_area :review %>
      <div class="new01">
      <%= f.submit '投稿', class:"btn btn-primary" %></div>
      <% end %>
    </div>
</div>

<script>
  var elem = document.getElementById('post_raty');
  var opt = {
      starOn: "<%= asset_path('star-on.png') %>",
      starOff: "<%= asset_path('star-off.png') %>",
      starHalf: "<%= asset_path('star-half.png') %>",
      scoreName: "review[rate]",
    };
  raty(elem,opt);
</script>